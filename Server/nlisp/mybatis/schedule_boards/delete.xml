<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="delete">
     <select id="deleteScheduleBoardData">
        DELETE FROM
            schedule_boards
        WHERE
        (
            id = #{id}
        AND
            (
                #{userId} = 1
            OR
                #{userId} = 2
            )
        )
     </select>
</mapper>