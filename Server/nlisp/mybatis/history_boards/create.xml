<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="create">
     <insert id="setHistoryBoardDatas">
        INSERT INTO
            history_boards
        (
            title,
            date,
            user_id
        )
        VALUES
        (
            #{title},
            #{date},
            #{id}
        )
     </insert>
</mapper>