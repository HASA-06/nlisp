<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="create">
     <insert id="setActivityBoardData">
        INSERT INTO
            activity_boards
        (
            title,
            content,
            image_url,
            image_name,
            user_id
        )
        VALUES
        (
            #{title},
            #{content},
            #{imageURL},
            #{imageName},
            #{userId}
        )
     </insert>
</mapper>